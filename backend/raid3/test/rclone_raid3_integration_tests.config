# rclone configuration file for raid3 integration tests
# Generated by: setup.sh
# Generated on: 2026-01-16 16:29:27 UTC
#
# This config file contains remotes for testing the raid3 backend.
# You can edit this file to customize remote names or paths.
# NOTE: Paths are relative to the test directory (backend/raid3/test)
#
# Local storage remotes
[localeven]
type = local

[localodd]
type = local

[localparity]
type = local

# RAID3 remote using local storage
[localraid3]
type = raid3
even = localeven:_data/even_local
odd = localodd:_data/odd_local
parity = localparity:_data/parity_local
timeout_mode = aggressive
auto_cleanup = true
auto_heal = false
use_streaming = true

# Single local remote (alias type)
[localsingle]
type = alias
remote = _data/single_local

# MinIO S3 remotes
[minioeven]
type = s3
provider = Minio
env_auth = false
access_key_id = even
secret_access_key = evenpass88
endpoint = http://127.0.0.1:9001
acl = private
no_check_bucket = false
max_retries = 1

[minioodd]
type = s3
provider = Minio
env_auth = false
access_key_id = odd
secret_access_key = oddpass88
endpoint = http://127.0.0.1:9002
acl = private
no_check_bucket = false
max_retries = 1

[minioparity]
type = s3
provider = Minio
env_auth = false
access_key_id = parity
secret_access_key = paritypass88
endpoint = http://127.0.0.1:9003
acl = private
no_check_bucket = false
max_retries = 1

# RAID3 remote using MinIO storage
[minioraid3]
type = raid3
even = minioeven:
odd = minioodd:
parity = minioparity:
timeout_mode = aggressive
auto_cleanup = true
auto_heal = false
use_streaming = true

# RAID3 remote using local and minio storage (mixed file/object backend)
[localminioraid3]
type = raid3
even = localeven:_data/even_local
odd = minioodd:
parity = localparity:_data/parity_local
timeout_mode = aggressive
auto_cleanup = true
auto_heal = false
use_streaming = true

[miniosingle]
type = s3
provider = Minio
env_auth = false
access_key_id = single
secret_access_key = singlepass88
endpoint = http://127.0.0.1:9004
acl = private
no_check_bucket = false
max_retries = 1
